<!DOCTYPE html>
<html>
<head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
<!--        <title>@ViewBag.Title - My ASP.NET Application</title>-->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="~/Content/Site.css">
</head>
<body>
        <div class="container-fluid">
                <div class="row row-gap-top-20">
                        <div class="col-sm-9 my-auto">
                                @Html.ActionLink("Dinucleotides calculation", "Index", "Home", new { style="color: black; font-size: 28px; padding: 20px 20px" })                                                        
                        </div>
                        <div class="col-sm-1 my-auto">
                                <li>@Html.ActionLink("Home", "Index", "Home", new { style = "color: black" })</li>
                        </div>
                        <div class="col-sm-1 my-auto">
                                <li>@Html.ActionLink("About", "About", "Home", new { style = "color: black" })</li>
                        </div>
                        <div class="col-sm-1 my-auto">
                                <li>@Html.ActionLink("Contacts", "Contacts", "Home", new { style = "color: black" })</li>
                        </div>
                </div>
        </div> 
                        
         <div class="container body-content" style="border:1px solid #cecece">
                @RenderBody()
        </div>
                
        <footer class="footer page-footer font-small black">
                 <div class="container text-center text-dark" style="position: ablolute; bottom:  0">
                        <a>&copy; @DateTime.Now.Year - Dinuc calculation</a>
                </div>
        </footer>
</body>
</html>

